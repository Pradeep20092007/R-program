prog 1:
Values=matrix(c(2,9,3,11,9,4,8,7,3,12,5,2,8,10,11),nrow=3,byrow=TRUE)
colors=c("red","yellow","red")
months=c("mar","apr","May","jan","jul")
regions=c("EAST","WEST","NORTH")
barplot(Values,main="Total Revenue",names.arg=months,xlab="Month",ylab="Revenue",col=colors,beside=TRUE)
legend("topleft",regions,cex=0.7,fill=colors)
grid(nx=NA,ny=NULL,col="grey",lty="dotdash")

