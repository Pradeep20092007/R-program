prog 3:
years=2001:2010
city_A=c(24,23,25,33,36,40,59,47,32,28)
city_B=c(25,30,27,32,40,30,50,28,30,46)
plot(years.city_A,type="o",col="blue",lwd=2,pch=16,xlab="Year",ylab="Number of Accidents",main="Number of Accidents in city A And city B",
ylim=c(20,65))
lines(years,city_B,type="o",col="red",lwd=2,pch=17)
legend("topright",legend=c("city A","city B"),col=c("blue","red")lty=1,pch=c(16,17),lwd=2)
grid(col="grey",lty="dotted")
